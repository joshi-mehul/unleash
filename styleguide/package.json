{
  "name": "web",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild-css": "mkdirp dist/css",
    "build-css": "postcss --use autoprefixer --use postcss-import --use csswring -d dist/css/ src/css/main.css",
    "build-pages": "node build/pages.js",
    "build": "npm run build-css && npm run build-pages && cp -r src/images/* dist/images/ && cp -r src/js/* dist/js/",
    "watch": "wr 'npm run build' src/**/*.{js,css} & ecstatic dist",
    "serve": "node serve.js",
    "lint": "standard src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "joshwnj",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.3.7",
    "basic-auth": "^1.0.4",
    "classnames": "^2.2.5",
    "csswring": "^5.1.1",
    "dateformat": "^2.0.0",
    "ecstatic": "^1.4.1",
    "glob": "^7.0.5",
    "mkdirp": "^0.5.1",
    "postcss": "^5.1.1",
    "postcss-cli": "^2.5.2",
    "postcss-import": "^8.1.2"
  },
  "devDependencies": {
    "browserify": "^14.1.0",
    "standard": "^7.1.2",
    "wr": "^1.3.1"
  }
}
